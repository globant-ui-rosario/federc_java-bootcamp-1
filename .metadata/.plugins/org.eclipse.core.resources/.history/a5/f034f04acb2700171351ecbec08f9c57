package keyPoint7;

public class AudioPlayer {
	private State state;
	
	private AudioPlayer(State state) {
	    this.state= state;
	  }
	  public void play() {
	    state.pressPlay(this);
	  }

	public State getState() {
		return state;
	}

	public void setState(State state) {
		this.state = state;
	}
	

}
